{
  "name": "web-viewer",
  "displayName": "Web Viewer",
  "description": "在 VS Code 中预览网页",
  "publisher": "hss",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-web-viewer.previewWebPage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-web-viewer.previewWebPage",
        "title": "预览网页"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/node": "^14.14.37",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hss0228/vscode-web-viewer"
  }
}
